package cms;

public enum Plan {
	SILVER(1000),GOLD(2000),DIAMOND(5000),PLATINUM(10000);
	
	private double charges;
	
	private Plan(double c) {
		charges = c;
	}
	public double getCharges() {
		return charges;
	}
	public void setCharges(double charges) {
		this.charges = charges;
	}
	public String toString() {
		return "[ "+name()+" : "+charges+" ]";
	}
}
