package utils;

import java.time.LocalDate;
import java.util.HashMap;

import StockTradingService.Stock;

public class StockUtils {
	public static HashMap<String, Stock> populateHashMap() {
		HashMap<String, Stock> EmptyMap = new HashMap<>();
		EmptyMap.put("APL",new Stock("APL", "Apple", "Apple Inc.", 1234.56, LocalDate.parse("2014-05-03"), 50));
		EmptyMap.put("TATAMTR",new Stock("TATAMTR", "TATA MOTORS", "TATA MOTORS Ltd.", 56.4321, LocalDate.parse("2016-11-09"), 70));
		EmptyMap.put("HP",new Stock("HP", "Hewlett Packard", "Hewlett Packard Ltd.", 111.22, LocalDate.parse("2007-01-08"), 20));
		return EmptyMap;
	}
}
