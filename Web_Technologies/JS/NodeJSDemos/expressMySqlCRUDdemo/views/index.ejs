//similar to JSP page
<!DOCTYPE html>
<html lang="en">
  <body>
    <header><h1>Product Management System</h1></header>
    <form action="/displayAddForm">
      <button type="submit" name="btn" id="btn" value="add">
        Add new Product
      </button>

    </form>
    <table>
        <thead>
        <tr>
            <th>Product ID </th>
            <th>Name</th>
            <th>Quantity</th>
            <th>Price</th>
        </tr>
    </thead>
    <tbody>
    <%for(var i =0;i<proddata.length;i++){ %>
        <tr>
            <td><%=proddata[i].pid %></td>
            <td><%=proddata[i].pname %></td>
            <td><%=proddata[i].qty %></td>
            <td><%=proddata[i].price %></td>
            <td><a href="#">Delete</a><a href="#">Edit</a></td>
        </tr>
    <% } %>
</tbody>
    </table>
</body>
</html>
